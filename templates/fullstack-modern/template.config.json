{
  "name": "fullstack-modern",
  "displayName": "Modern Full-Stack Template",
  "description": "Production-ready full-stack application with React 18, Next.js 14, Rust backend, real-time features, and modern deployment setup",
  "version": "1.0.0",
  "type": "fullstack",
  "framework": "nextjs",
  "language": "typescript",
  "backend": {
    "language": "rust",
    "framework": "axum"
  },
  "features": [
    "typescript",
    "react-18",
    "nextjs-14",
    "rust-backend",
    "supabase-auth",
    "real-time",
    "websockets",
    "zustand",
    "tanstack-query",
    "shadcn-ui",
    "tailwindcss",
    "framer-motion",
    "vercel-deployment",
    "docker",
    "ci-cd",
    "monitoring",
    "error-handling"
  ],
  "dependencies": {
    "frontend": [
      "next@^14.0.4",
      "react@^18.2.0",
      "react-dom@^18.2.0",
      "@supabase/supabase-js@^2.38.5",
      "@tanstack/react-query@^5.17.15",
      "zustand@^4.4.7",
      "framer-motion@^10.16.16",
      "react-hook-form@^7.48.2",
      "zod@^3.22.4"
    ],
    "backend": [
      "axum@0.7",
      "tokio@1.0",
      "sqlx@0.7",
      "redis@0.24",
      "serde@1.0",
      "jsonwebtoken@9.2",
      "uuid@1.6"
    ]
  },
  "devDependencies": {
    "frontend": [
      "typescript@^5.3.3",
      "@types/node@^20.10.6",
      "@types/react@^18.2.46",
      "tailwindcss@^3.4.0",
      "eslint@^8.56.0",
      "prettier@^3.1.1",
      "jest@^29.7.0",
      "@playwright/test@^1.40.1"
    ],
    "backend": [
      "cargo-watch",
      "cargo-audit"
    ]
  },
  "scripts": {
    "frontend": {
      "dev": "next dev",
      "build": "next build",
      "start": "next start",
      "lint": "next lint",
      "test": "jest",
      "test:e2e": "playwright test"
    },
    "backend": {
      "dev": "cargo watch -x run",
      "build": "cargo build --release",
      "test": "cargo test",
      "lint": "cargo clippy",
      "format": "cargo fmt"
    },
    "docker": {
      "dev": "docker-compose up -d",
      "build": "docker-compose build",
      "down": "docker-compose down"
    }
  },
  "structure": {
    "frontend/": "Next.js React application",
    "backend/": "Rust API server with Axum",
    "shared/": "Shared types and utilities",
    "docker-compose.yml": "Development environment",
    "vercel.json": "Vercel deployment configuration",
    ".github/workflows/": "CI/CD pipeline",
    ".env.example": "Environment variables template"
  },
  "configs": {
    "frontend/next.config.js": "Next.js configuration with security headers",
    "frontend/tailwind.config.js": "Tailwind CSS with custom design system",
    "frontend/tsconfig.json": "TypeScript configuration",
    "backend/Cargo.toml": "Rust dependencies and project config",
    "backend/src/main.rs": "Main Rust application entry point",
    "docker-compose.yml": "Multi-service development environment"
  },
  "agents": [
    "frontend-specialist",
    "backend-engineer", 
    "database-architect",
    "devops-engineer",
    "security-auditor"
  ],
  "mcpServers": [
    "filesystem",
    "postgres",
    "redis", 
    "docker",
    "github",
    "vercel",
    "supabase"
  ],
  "bestPractices": {
    "linting": true,
    "testing": true,
    "security": true,
    "documentation": true,
    "cicd": true,
    "monitoring": true,
    "errorHandling": true,
    "realTime": true,
    "authentication": true,
    "deployment": true
  },
  "deployment": {
    "frontend": {
      "platform": "vercel",
      "buildCommand": "npm run build",
      "outputDirectory": ".next",
      "env": [
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY"
      ]
    },
    "backend": {
      "platform": "railway",
      "buildCommand": "cargo build --release",
      "startCommand": "./target/release/app",
      "env": [
        "DATABASE_URL",
        "REDIS_URL",
        "JWT_SECRET",
        "SUPABASE_SERVICE_ROLE_KEY"
      ]
    }
  },
  "quickStart": [
    "npx claude-flow@2.0.0 create my-app --template fullstack-modern",
    "cd my-app",
    "cp .env.example .env",
    "# Configure environment variables",
    "docker-compose up -d",
    "# Frontend: http://localhost:3000",
    "# Backend: http://localhost:8000",
    "# Database: http://localhost:8080 (Adminer)"
  ],
  "documentation": {
    "setup": "./README.md",
    "deployment": "./docs/DEPLOYMENT.md",
    "api": "./docs/API.md",
    "database": "./docs/DATABASE.md"
  }
}