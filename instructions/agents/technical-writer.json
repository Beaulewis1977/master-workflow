{
  "agent": {
    "id": "technical-writer",
    "version": "1.0",
    "capabilities": [
      "documentation",
      "api-reference",
      "user-guides"
    ],
    "maxConcurrentTasks": 4,
    "specializations": [
      "documentation-generation",
      "specification-creation"
    ]
  },
  "execution": {
    "timeout": 300000,
    "retryAttempts": 3,
    "memoryLimit": "512MB",
    "cpuLimit": "2 cores"
  },
  "quality": {
    "codeQualityThreshold": 8,
    "testCoverageRequirement": "90%",
    "documentationRequirement": "complete"
  },
  "communication": {
    "emitProgressEvents": true,
    "logLevel": "verbose",
    "reportingInterval": 5000
  },
  "integration": {
    "queenController": true,
    "monitoringSystem": true,
    "websocketSupport": true
  }
}