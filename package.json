{
  "name": "master-workflow",
  "version": "3.0.0",
  "description": "Master Workflow 3.0 - Unified AI Development Platform combining Claude Flow, Agent OS, Hive-Mind, and revolutionary intelligence systems",
  "main": "src/index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "node src/index.js",
    "demo": "node demos/master-workflow-3.0-demo.js",
    "install:interactive": "node src/installer/interactive-installer.js",
    "install:enhanced": "node src/installer/enhanced-installer.js",
    "flow": "node src/cli/flow-cli.js",
    "agent": "node src/cli/agent-cli.js",
    "build:app": "node src/builder/autonomous-builder.js",
    "test": "node test/test-runner.js",
    "test:new": "node --test src/**/*.test.js",
    "test:interactive": "node test/run-interactive-ai-tests.js",
    "test:interactive:quick": "node test/run-interactive-ai-tests.js --mode quick",
    "test:interactive:comprehensive": "node test/run-interactive-ai-tests.js --mode comprehensive",
    "test:ai-enhancement": "node test/run-interactive-ai-tests.js --mode individual --suites ai-enhancement",
    "test:fullstack-modern": "node test/run-interactive-ai-tests.js --mode individual --suites fullstack-modern",
    "lint": "node scripts/lint-if-available.js",
    "coverage": "echo 'No coverage tool configured; skipping'",
    "install-sqlite": "npm install sqlite3 sqlite || echo 'SQLite installation failed, will use file-based fallback'",
    "check-deps": "node lib/dependency-manager/claude-flow-deps.js check",
    "install-deps": "node lib/dependency-manager/claude-flow-deps.js install",
    "verify-deps": "node lib/dependency-manager/claude-flow-deps.js verify",
    "test-deps": "node lib/dependency-manager/test/dependency-manager-test.js",
    "dev": "node --watch src/index.js",
    "bootstrap": "node -e \"import('./src/bootstrap/project-bootstrap-agent.js').then(m => new m.ProjectBootstrapAgent({interactive:true}).bootstrap())\"",
    "wiki": "node -e \"import('./src/bootstrap/project-bootstrap-agent.js').then(m => new m.ProjectBootstrapAgent().then(a => a._createRepoWiki()))\"",
    "agentdb:status": "node -e \"import('./src/claude-flow/agentdb-integration.js').then(m => {const db=new m.AgentDB();db.initialize().then(()=>console.log(db.getStats()))})\"",
    "compactor:status": "node -e \"import('./src/context/context-compactor.js').then(m => console.log(new m.ContextCompactor().getStatus()))\"",
    "skills:list": "node -e \"import('./src/claude-flow/skills-system.js').then(m => new m.SkillsSystem().getSkills().forEach(s=>console.log(s.name)))\"",
    "system:status": "node -e \"import('./src/index.js').then(m => {const s=new m.MasterWorkflow3();s.initialize().then(()=>s.getStatus().then(console.log))})\"",
    "marketplace:start": "node src/marketplace/agent-registry-api.js",
    "marketplace:test": "node src/marketplace/test-api.js",
    "marketplace:example": "node src/marketplace/api-client-example.js",
    "setup": "npm install && npm run install-sqlite && echo 'âœ… Setup complete! Run: npm run install:enhanced'",
    "quick-start": "npm run setup && npm run install:enhanced"
  },
  "keywords": [
    "ai",
    "claude",
    "workflow",
    "autonomous",
    "agent-os",
    "claude-code",
    "claude-flow",
    "hive-mind",
    "quantum-memory",
    "neural-swarm",
    "app-builder"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "optionalDependencies": {
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "dependencies": {
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "archiver": "^7.0.1",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^11.1.0",
    "diff": "^8.0.2",
    "dotenv": "^16.3.1",
    "inquirer": "^9.3.7",
    "ml-matrix": "^6.12.1",
    "ora": "^8.0.1",
    "tar": "^7.4.3",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "chart.js": "^4.5.0",
    "node-fetch": "^3.3.2",
    "ws": "^8.18.3"
  }
}
